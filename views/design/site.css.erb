<%
    def html2rgb(html_color, alpha=1)
        rgb = Color::RGB.from_html(html_color)
        red = (rgb.r * 255).to_i
        green = (rgb.g * 255).to_i
        blue = (rgb.b * 255).to_i
        "rgba(#{red},#{green},#{blue},#{alpha})"
    end
%>

