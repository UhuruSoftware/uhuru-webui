    text-align: left;
    list-style: none;
    display: block;
    border-bottom: 1px solid <%= design['tab_menu']['border-color'] %>;
    left: -30px;
    color: <%= design['tab_menu']['font-color'] %>;
}

<% design['link'].each do |style, color| %>
.tab_menu a:<%= style %>
{
    color: <%= color %>;
}
<% end %>

.tab_menu ul li
{
    text-align: left;
    display: inline-block;
    position: relative;
    padding: 5px;
    top: 1px;
    cursor: pointer;
}

