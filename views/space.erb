<div class="sections" id="space_page">

        <div class="manage_content_header">
            <h2 class="section_title">
                <span id="space_name"><%= $currentSpace_Name %></span>

                <form action="/updateSpace" method="post">
                    <input type="text" value="" id="space_input" maxlength="20" name="m_spaceName"/>
                    <input type="submit" style="display: none; width: 1px; height: 1px;">
                </form>

            </h2>
            <div class="icons_options">
                <input type="button" value="" class="plus_sign_48 plus_sign_position_48" id="create_app" />
                <input type="button" value="" class="plus_sign_48 plus_sign_position_48" id="create_service" style="display: none;" />
                <input type="button" value="" class="plus_sign_48 plus_sign_position_48" id="button_add_subscription" style="display: none;" />

                <input type="button" value="" class="edit_sign_48 edit_sign_position_48" onclick="showSpacesInput();" />
                <input type="button" value="" class="money_bag_sign_48 money_bag_sign_position_48" />
                <input type="button" class="trash_sign_48 trash_sign_position_48 delete_this_space" value="" />
            </div>
        </div>

