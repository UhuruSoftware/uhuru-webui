                                        <span class="app_state blank_input" id="<%= app.state %>"></span>
                                        <span class="app_instances blank_input" id="<%= app.instances %>"></span>
                                        <span class="app_memory blank_input" id="<%= app.memory %>"></span>
                                    </a>

                                    <span class="small_trash delete-app-button" id="<%= app.name %>" title="<%= app.name %>" onmousemove="prepareToDeleteApp(this);"></span>
